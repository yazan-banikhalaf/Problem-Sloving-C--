#include<bits/stdc++.h>

using namespace std;

int main()
{
    
    int x,y,z;
    cin >> x >> y >>z;
    
    if(x <= y && x <= z) {
        cout << x << "\n" << min(y,z) << "\n" << max(y,z);
    } else if(y <= x && y <= z) {
        cout << y << "\n" << min(x,z) << "\n" << max(x,z);
    } else {
        cout << z << "\n" << min(x,y) << "\n" << max(x,y);
    }
    
    cout << endl;
    cout << endl;
    
    cout << x << "\n" << y << "\n" << z;
    return 0;
}
