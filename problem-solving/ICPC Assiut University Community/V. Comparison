#include <iostream>
using namespace std;

int main()
{
    int x , y;
    char op;
    
    cin >> x >> op >> y;
    
    switch(op) {
        case '<':
        x < y ? cout <<"Right" : cout << "Wrong";
        break;
        case '>':
        x > y ? cout << "Right" : cout << "Wrong";
        break;
        case '=':
        x == y ? cout << "Right" : cout << "Wrong";
        break;
        
        default:
        cout <<"Wrong";
    }
    return 0;
}
