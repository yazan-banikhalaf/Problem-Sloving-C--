#include <iostream>
using namespace std;

int main()
{
    int age;
    cin >> age;
    
    int years = age / 365;
    int months = (age - (years * 365))/30  ;
    int days = age - ((years * 365) + (months * 30));
    cout << years << " years" << endl;
    cout << months << " months" << endl;
    cout << days << " days" << endl;
    return 0;
}
