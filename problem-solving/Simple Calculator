#include <iostream>
using namespace std;

int main()
{
    
    long long x , y;
    
    cin >> x >> y;
    
    int add = x + y;
    
    long long multi = x * y;
    
    int sub = x - y;
    
    cout << x << " + " << y << " = " << add << endl;
    
    cout << x << " * " << y << " = " << multi << endl;
    
    cout << x << " - " << y << " = " << sub << endl;
    
    return 0;
}
