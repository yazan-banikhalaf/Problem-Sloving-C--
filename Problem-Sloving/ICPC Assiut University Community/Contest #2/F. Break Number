#include <iostream>
using namespace std;

int main() {
    int n;
    cin >> n;
    
    int maxDivCount = 0; 


    for (int i = 0; i < n; i++) {
        long long number;
        cin >> number;

        int divCount = 0;

        while (number % 2 == 0) {
            number /= 2;
            divCount++;
        }

        maxDivCount = max(maxDivCount, divCount);
        cout << maxDivCount << endl;
    }


    return 0;
}
