#include<bits/stdc++.h>

using namespace std;

int main()
{
    char c;
    
    
    cin >> c;
    int ascii = c;

    for(int i = 97; i <= 121; i++) {
        if(ascii == i) {
            ascii++;
            cout << char(ascii);
            break;
        } else if(ascii == 122) {
            cout << 'a' << endl;
            break;
        } 
    }
    return 0;
}
