#include <iostream>
#include <vector>

using namespace std;

int main() {
    
    int CorrectPassword = 1999;
    int GuessPassword;
    
    
    while(true) {
        cin >> GuessPassword;
        if(GuessPassword == CorrectPassword) {
            cout << "Correct\n";
            break;
        } else {
            cout << "Wrong\n";
        }
    }
    return 0;
}
