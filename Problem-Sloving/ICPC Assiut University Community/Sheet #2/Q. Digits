#include <bits/stdc++.h>
using namespace std;

int main()
{
    int n;
    
    
    cin >> n;
    
    for(int i = 0; i < n; i++) {
        string number;
        cin >> number;
        for(int i = number.size()-1; i >= 0; i--) {
            cout << number[i] << " ";    
        }
        cout << endl;
    }
    return 0;
}
