#include<bits/stdc++.h>
using namespace std;


int main() {
    
    char S;
    int counter;
    cin >> S >> counter;
    for(int i = 1; i <= counter; i++) {
        int numbers;
        cin >> numbers;
        for(int j = 1; j <= numbers; j++) {
            cout << S;
        }
        cout << endl;
    }
    return 0;
}
