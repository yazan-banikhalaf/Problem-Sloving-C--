#include <iostream>
using namespace std;

int main()
{
    double x;
    cin >> x;
    
    int xx;
    
    xx = x;

    int number = xx % 1000;
    
    float rem;
    
    rem = x - number;
    
    rem == 0 ? cout << "int "  <<number : cout << "float " << number << " " << rem; 
  
    return 0;
}
