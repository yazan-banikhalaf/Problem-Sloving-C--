#include <iostream>
using namespace std;

int main()
{
    int x , y , z;
    int sum = 0;
    char op, eq;
    
    cin >> x >> op >> y >> eq >> z;
    
    switch(op) {
        case '+':
        sum = x + y;
        sum == z ? cout << "Yes" : cout << sum;
        break;
        
        case '-':
        sum = x - y;
        sum == z ? cout << "Yes" : cout << sum;
        break;
        
        case '*':
        sum = x * y;
        sum == z ? cout << "Yes" : cout << sum;
        break;
        
        default:
        cout <<"Wrong";
    }
    return 0;
}
