#include <iostream>
#include <math.h>
using namespace std;

int main()
{
    double x, y, z;
    
    cin >> x >> y;
    
    z = x / y;
    
    cout << "floor " << x << " / " << y << " = " << floor(z) << endl;
    cout << "ceil  " << x << " / " << y << " = " << ceil(z) << endl;
    cout << "round  " << x << " / " << y << " = " << round(z) << endl;
    
    return 0;
}
